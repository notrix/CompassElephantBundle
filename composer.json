        {
            "name": "Matteo Giachino",
            "email": "matteog@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "notrix/compass-elephant": "dev-master",
        "symfony/http-kernel": "~2.3|~3.0",
